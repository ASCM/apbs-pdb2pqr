    </BuildMacro>
    <BuildMacro Include="MALOC_INCLUDES">
      <Value>$(MALOC_INCLUDES)</Value>
    </BuildMacro>
    <BuildMacro Include="ZLIB_INCLUDES">
      <Value>$(ZLIB_INCLUDES)</Value>
    </BuildMacro>
    <BuildMacro Include="APBS_INCLUDES">
      <Value>$(APBS_INCLUDES)</Value>
    </BuildMacro>
    <BuildMacro Include="ALL_INCLUDES">
      <Value>$(ALL_INCLUDES)</Value>
    </BuildMacro>
  </ItemGroup>
