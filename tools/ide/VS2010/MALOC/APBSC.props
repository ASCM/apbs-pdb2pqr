    <BuildMacro Include="CONF_INCLUDES">
      <Value>$(CONF_INCLUDES)</Value>
    </BuildMacro>
    <BuildMacro Include="TOP_SRCDIR">
      <Value>$(TOP_SRCDIR)</Value>
